<idea-plugin>
    <id>com.ahmedmourad.bug</id>
    <name>Test Compiler</name>

    <description><![CDATA[
    A test IDEA plugin made specifically to showcase the bug with the inspection suppressor
    ]]></description>

    <depends>org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">
        <lang.inspectionSuppressor
                language="kotlin"
                implementationClass="com.ahmedmourad.buggy.idea.DataClassPrivateConstructorInspectionSuppressor"
        />
    </extensions>

</idea-plugin>
